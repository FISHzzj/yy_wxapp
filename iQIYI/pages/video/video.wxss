.main {
    width: 100%;
    height: 100vh;
}

.video-tab-box {
    height: 101rpx;
    width: 100%;
    position: relative;
}

.video-tab-box view {
    display: inline-block;
    width: 25%;
    line-height: 101rpx;
    text-align: center;
}

.line {
    display: inline-block;
    width: 20px;
    height: 3px;
    background-color: green;
    position: absolute;
    bottom: 0;
}

.video-list {
    height: 532rpx;
    width: 100%;
    /* display: flex; */
    flex-direction: column;
}

.wrap-video {
    height: 422rpx;
    width: 100%;
    position: relative;
}

.wrap-video .video-title {
    position: absolute;
    z-index: 999;
    color: #fff;
    margin: 5rpx 0 0 5rpx;
}

.wrap-video video {
    height: 100%;
    width: 100%;
}

.video-option {
    height: 110rpx;
}

.video-option view {
    display: inline-block;
    line-height: 110rpx;
}

.video-option view text {
    margin: 0 10rpx;
}

.video-option view.watch-num {
    float: left;
}

.video-option view.share,
.video-option view.like {
    float: right;
}

.video-tab-nav.on {
    /* background-color: green; */
}

@font-face {
    font-family: "iconfont";
    src: url('iconfont.eot?t=1527699692613');
